<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
	  <!-- Inherit Form View to Modify it -->
        <record id="partner_view_contact_link" model="ir.ui.view">
            <field name="name">res.partner.link.form</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@class='oe_module_desc']" position="inside">
                    <button name="go_to_partner" type="object"
                                icon="terp-gtk-jump-to-ltr"
                                string="Go to Partner"/>
                </xpath>
            </field>
        </record>
    </data>
</openerp>
