<openerp>
    <data>
        <template id="index" inherit_id='point_of_sale.index' name="Point Of Sale Table Index">&lt;!DOCTYPE html&gt;
            <xpath expr="//link[@id='pos-stylesheet']" position="after">
                <link rel="stylesheet" href="/my_pos_rounding/static/src/css/pos.css" />
            </xpath>
        </template>

        <template id="assets_backend" name="my_pos_rounding_assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/pos_rounding/static/src/js/pos_rounding.js"></script>
            </xpath>
        </template>
    </data>
</openerp>